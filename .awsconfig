# ebcli
function eb() {
  if [[ $1 == 'printenv' ]]; then
    /usr/bin/env eb printenv "${@:2}" | sed -e "s/^[ \t]*//;s/ = \(.*\)/='\1'/;/^Environment.*/d" | sort
  else
    /usr/bin/env eb "$@"
  fi
}

# aws-shell using aws-vault
alias msr-shell='aws-vault exec ryan-msr-admin -- aws-shell'
alias amzn-shell='aws-vault exec ryan-msr-amzn -- aws-shell'
