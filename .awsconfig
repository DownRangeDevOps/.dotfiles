# vim: set ft=sh:

# ebcli
function eb() {
  if [[ $1 == 'printenv' ]]; then
    /usr/bin/env eb printenv "${@:2}" | sed -e "s/^[ \t]*//;s/ = \(.*\)/='\1'/;/^Environment.*/d" | sort
  else
    /usr/bin/env eb "$@"
  fi
}

# aws-vault
function ave() {
    aws-vault exec ${*}
}
function ave-sbx() {
    aws-vault exec ryan-msr-ops-sbx -- ${*}
}
