# Includes
Include ~/.ssh/personal/config
Include ~/.ssh/grainger/config

# Global
Host *
  ForwardAgent no
  ForwardX11 no
  AddKeysToAgent yes
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
  IdentitiesOnly yes

Host bastion.startupwerx.com
  Hostname bastion.startupwerx.com
  ForwardAgent no
  ForwardX11 no
  AddKeysToAgent yes
  User ec2-user
  IdentityFile ~/.ssh/startupwerx_id_ed25519
  IdentitiesOnly yes

Host app.startupwerx.com
  Hostname ip-10-10-21-195.us-west-1.compute.internal
  ProxyJump bastion.startupwerx.com
  ForwardAgent no
  ForwardX11 no
  AddKeysToAgent yes
  User ec2-user
  IdentityFile ~/.ssh/startupwerx_id_ed25519
  # IdentityFile ~/.ssh/stuwx-uw1-prod-asg-ssh-key
  IdentitiesOnly yes
